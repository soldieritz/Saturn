
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<% if(success.length > 0) { %> 
<script>
Swal.fire(
  'Exito!',
  '<%= success %>',
  'success'
)
</script>
<% } %>

 <% if(error.length > 0) { %> 
  <script>
Swal.fire({
  icon: 'error',
  title: 'Oops...',
  text: '<%= error %>',
  footer: '<a href="">Reportar bug?</a>'
})
    </script>
<% } %>

<% if(addbot_errors.length > 0) {%>
  <% let t = [] %>
  <% addbot_errors.forEach(err => { %>
   <% t.push(err) %>
  <% }) %>
  <% let y = `[${t.join("] [")}]` %>
<script>
  Swal.fire({
      icon: 'error',
      title: 'Oops...',
      html: 'Porfavor ingresa correctamente los parametros. <strong> <%= y %> <strong>',
      footer: '<a href="">Reportar Bug?</a>'
    })
</script>
<% } %>

<% errors.forEach(err => {%>
  <div class="container" >
      <div class="alert alert-danger" style="padding: 10px; border: 0px" role="alert">
          <%- err %>
        </div>
   </div>
<% }) %> 
